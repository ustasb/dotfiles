{
  "coc.preferences.timeout": 4000,
  "coc.preferences.autoTrigger": "none",
  "coc.preferences.minTriggerInputLength": 2,
  "coc.preferences.maxCompleteItemCount": 15,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.diagnostic.displayByAle": true,
  "coc.preferences.triggerCompletionWait": 300,

  "css.enable": true,
  "json.enable": true,
  "coc.source.dictionary.enable": true,
  "coc.source.tag.enable": true,
  "coc.source.ultisnips.enable": true,
  "solargraph.enable": true,
  "tsserver.enable": true,

  "eslint.enable": true,
  "eslint.run": "onSave",

  "pyls.enable": true,
  "pyls.configurationSources": ["pycodestyle"],
  "pyls.plugins.pycodestyle.maxLineLength": 100
}
